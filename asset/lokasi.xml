<odoo>
	<data>
		<record id="view_management_lokasi_tree" model="ir.ui.view">
			<field name="name">management.lokasi.tree</field>
			<field name="model">management.lokasi</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="lokasi">
					<!-- <field name="no"/> -->
					<field name="no_lokasi"/>
					<field name="lokasi"/>
					<!-- <field name="action"/> -->
				</tree>
			</field>
		</record>
		<record id="view_management_lokasi_form" model="ir.ui.view">
			<field name="name">management.lokasi.form</field>
			<field name="model">management.lokasi</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="lokasi Form">
					<sheet>
						<div class="oe_title">
                            <label for="no_lokasi" class="oe_edit_only" string="No Lokasi"/>
                            <h1>
                                <field name="no_lokasi"/>
                            </h1>
                        </div>
                        <group>
                            <field name="lokasi"/>
                        </group>
                    </sheet>
				</form>
			</field>
		</record>	
	</data>
</odoo>