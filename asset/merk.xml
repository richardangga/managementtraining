<odoo>
	<data>
		<record id="view_management_merk_tree" model="ir.ui.view">
			<field name="name">management.merk.tree</field>
			<field name="model">management.merk</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="merk">
					<field name="nama_merk"/>
				</tree>
			</field>
		</record>
		<record id="view_management_merk_form" model="ir.ui.view">
			<field name="name">management.merk.form</field>
			<field name="model">management.merk</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="merk Form">
					<sheet>
						<div class="oe_title">
							 <label for="nama_merk" class="oe_edit_only" string="Nama Merk"/>
                            <h1>
                                <field name="nama_merk"/>
                            </h1>
                        </div>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>