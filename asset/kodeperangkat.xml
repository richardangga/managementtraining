<odoo>
	<data>
		<record id="view_management_kodeperangkat_tree" model="ir.ui.view">
			<field name="name">management.kodeperangkat.tree</field>
			<field name="model">management.kodeperangkat</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="kodeperangkat">
					<!-- <field name="id_subunit"/> -->
					<field name="no_perangkat"/>
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="view_management_kodeperangkat_form" model="ir.ui.view">
			<field name="name">management.kodeperangkat.form</field>
			<field name="model">management.kodeperangkat</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="kodeperangkat Form">
					<sheet>
						<div class="oe_title">
                            <label for="no_perangkat" class="oe_edit_only" string="No Perangkat"/>
                            <h1>
                                <field name="no_perangkat"/>
                            </h1>
                        </div>
                        <group>
                            <!-- <field name="no_subunit"/> -->
                            <field name="name"/>
                        </group>
                    </sheet>
				</form>
			</field>
		</record>	
	</data>
</odoo>