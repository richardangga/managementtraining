<odoo>
	<data>
		<record id="view_management_kodegolperangkat_tree" model="ir.ui.view">
			<field name="name">management.kodegolperangkat.tree</field>
			<field name="model">management.kodegolperangkat</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="kodegolperangkat">
					<field name="id_kelperangkat"/>
					<field name="no_kelperangkat"/>
					<field name="nama_kelperangkat"/>
				</tree>
			</field>
		</record>
		<record id="view_management_kodegolperangkat_form" model="ir.ui.view">
			<field name="name">management.kodegolperangkat.form</field>
			<field name="model">management.kodegolperangkat</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="kodegolperangkat Form">
					<sheet>
						<div class="oe_title">
                            <label for="id_kelperangkat" class="oe_edit_only" string="Kode Golongan Perangkat"/>
                            <h1>
                                <field name="id_kelperangkat"/>
                            </h1>
                        </div>
                        <group>
                            <field name="no_kelperangkat"/>
                            <field name="nama_kelperangkat"/>
                        </group>
                    </sheet>
				</form>
			</field>
		</record>	
	</data>
</odoo>